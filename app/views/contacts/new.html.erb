<%= form_for :contacts do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    
    <%= f.label :phone %>
    <%= f.text_field :phone %>
    
    <%= f.label :email %>
    <%= f.email_field :email %>
    
    <%= f.label :event_type %>
    <%= f.text_field :event_type %>
    
    <%= f.label :comments %>
    <%= f.text_field :comments %>
    
    <%= f.submit "Submit" %>
<% end %>